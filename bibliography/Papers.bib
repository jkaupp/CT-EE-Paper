title = {{Using changes in hydrostatic and osmotic pressure to manipulate metabolic function in chondrocytes.}},
journal = {American journal of physiology. Cell physiology},
year = {2011},
volume = {300},
number = {6},
pages = {C1234--45},
month = jun,
affiliation = {Orthopedic Research, Brigham and Women's Hospital, 75 Francis St., Boston, MA 02115, USA. smizuno@rics.bwh.harvard.edu},
doi = {10.1152/ajpcell.00309.2010},
pmid = {21270297},
language = {English},
rating = {0},
date-added = {2013-09-17T01:43:01GMT},
date-modified = {2015-03-17T13:42:29GMT},
abstract = {Articular cartilage has distinct histological depth zones. In each zone, chondrocytes are subject to different hydrostatic (HP) and osmotic pressure (OP) due to weight-bearing and joint-loading. Previous in vitro studies of regeneration and pathophysiology in cartilage have failed to consider the characteristics of histological heterogeneity and the effects of combinations of changes in HP and OP. Thus, we have constructed molecular, biochemical, and histological profiles of anabolic and catabolic molecules produced by chondrocytes from each depth zone isolated from bovine articular cartilage in response to changes in HP and OP. We cultured the chondrocytes with combinations of loading or off-loading of HP at 0-0.5 MPa, 0.5 Hz, and changes in OP of 300-450 mosM over 1 wk, and evaluated mRNA expression and immunohistology of both anabolic and catabolic molecules and amounts of accumulated sulfated glycosaminoglycan. Any changes in HP and OP upregulated mRNA of anabolic and catabolic molecules in surface-, middle-, and deep-zone cells, in descending order of magnitude. Off-loading HP maintained the anabolic and reduced the catabolic mRNA; high OP retained upregulation of catabolic mRNA. These molecular profiles were consistent with immunohistological and biochemical findings. Changes in HP and OP are essential for simulating chondrocyte physiology and useful for manipulating phenotypes.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=21270297&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Jake/Dropbox/Library.papers3/Articles/2011/Mizuno/Am.%20J.%20Physiol.%20Cell%20Physiol.%202011%20Mizuno.pdf},
file = {{Am. J. Physiol. Cell Physiol. 2011 Mizuno.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/2011/Mizuno/Am. J. Physiol. Cell Physiol. 2011 Mizuno.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1152/ajpcell.00309.2010}}
}

