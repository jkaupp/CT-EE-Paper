title = {{Increased friction coefficient and superficial zone protein expression in patients with advanced osteoarthritis.}},
journal = {Arthritis and rheumatism},
year = {2010},
volume = {62},
number = {9},
pages = {2680--2687},
month = sep,
affiliation = {Purdue University, West Lafayette, Indiana 47097-2032, USA. cpneu@purdue.edu},
doi = {10.1002/art.27577},
pmid = {20499384},
