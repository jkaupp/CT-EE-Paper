title = {{Low-density cultures of bovine chondrocytes: effects of scaffold material and culture system.}},
journal = {Biomaterials},
year = {2005},
volume = {26},
number = {14},
pages = {2001--2012},
month = may,
affiliation = {Department of Bioengineering, Rice University, Mail Stop 142, 6100 Main, Houston, TX 77005, USA.},
doi = {10.1016/j.biomaterials.2004.06.038},
pmid = {15576174},
language = {English},
rating = {0},
date-added = {2013-09-17T01:41:54GMT},
date-modified = {2015-03-17T13:42:38GMT},
abstract = {Chondrocytes were seeded on either agarose or polyglycolic acid (PGA) unwoven meshes at 10 million cells/ml of scaffold volume to evaluate the effect that these two biomaterials have on the low-density culture of chondrocytes in a rotating-wall bioreactor. For both static and bioreactor culture, agarose constructs contained more glycosaminoglycan than their PGA counterparts. However, the PGA constructs contained more collagen for both culture conditions when compared to agarose. For the low seeding density of this study, PGA constructs cultured in the bioreactor did not outperform static cultures when comparing collagen content after 8 weeks. The mechanical properties of the PGA constructs also did not improve with culture time. Similar results were observed with the agarose culture, though both static- and bioreactor-culture agarose constructs exhibited increases in aggregate modulus at the end of the culture period. As in PGA culture, chondrocytes cultured in agarose may require a higher density to reap the benefits of the bioreactor environment.},
url = {http://www.sciencedirect.com/science/article/pii/S0142961204006003},
local-url = {file://localhost/Users/Jake/Dropbox/Library.papers3/Articles/2005/HU/Biomaterials%202005%20HU.pdf},
file = {{Biomaterials 2005 HU.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/2005/HU/Biomaterials 2005 HU.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.biomaterials.2004.06.038}}
}

@article{Bruckner:1989ue,
author = {Bruckner, Peter and H{\"o}rler, I and Mendler, M and Houze, Y and Winterhalter, K H and Eich-Bender, S G and Spycher, M A},
title = {{Induction and prevention of chondrocyte hypertrophy in culture.}},
journal = {The Journal of cell biology},
year = {1989},
volume = {109},
number = {5},
pages = {2537--2545},
month = nov,
affiliation = {Laboratorium f{\"u}r Biochemie, Eidgen{\"o}ssische Technische Hochschule, Z{\"u}rich, Switzerland.},
pmid = {2808534},
pmcid = {PMC2115829},
language = {English},
rating = {0},
date-added = {2013-09-17T01:42:40GMT},
date-modified = {2015-03-17T13:42:38GMT},
abstract = {Primary chondrocytes from whole chick embryo sterna can be maintained in suspension culture stabilized with agarose for extended periods of time. In the absence of FBS, the cells remain viable only when seeded at high densities. They do not proliferate at a high rate but they deposit extracellular matrix with fibrils resembling those of authentic embryonic cartilage in their appearance and collagen composition. The cells exhibit many morphological and biochemical characteristics of resting chondrocytes and they do not produce collagen X, a marker for hypertrophic cartilage undergoing endochondral ossification. At low density, cells survive in culture without FBS when the media are conditioned by chondrocytes grown at high density. Thus, resting cartilage cells in agarose cultures can produce factors required for their own viability. Addition of FBS to the culture media leads to profound changes in the phenotype of chondrocytes seeded at low density. Cells form colonies at a high rate and assume properties of hypertrophic cells, including the synthesis of collagen X. They extensively deposit extracellular matrix resembling more closely that of adult rather than embryonic cartilage.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=2808534&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Jake/Dropbox/Library.papers3/Articles/1989/Bruckner/J.%20Cell%20Biol.%201989%20Bruckner.pdf},
file = {{J. Cell Biol. 1989 Bruckner.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/1989/Bruckner/J. Cell Biol. 1989 Bruckner.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/296FD447-5C50-4B00-968E-72420AD6502A}}
}

