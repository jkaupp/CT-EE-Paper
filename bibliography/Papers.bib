author = {Hunter, Christopher J and Mouw, Janna K and Levenston, Marc E},
title = {{Dynamic compression of chondrocyte-seeded fibrin gels: effects on matrix accumulation and mechanical stiffness.}},
journal = {Osteoarthritis and Cartilage},
year = {2004},
volume = {12},
number = {2},
