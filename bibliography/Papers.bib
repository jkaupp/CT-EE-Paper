@article{Deschner:2003ff,
author = {Deschner, James and Hofman, Cynthia R and Piesco, Nicholas P and Agarwal, Sudha},
title = {{Signal transduction by mechanical strain in chondrocytes.}},
journal = {Current opinion in clinical nutrition and metabolic care},
year = {2003},
volume = {6},
number = {3},
pages = {289--293},
month = may,
affiliation = {Department of Oral Medicine and Pathology, University of Pittsburgh School of Dental Medicine, Pittsburgh, Philadelphia 15261-1964, USA.},
keywords = {Chondrocyte Strain},
doi = {10.1097/01.mco.0000068964.34812.2b},
pmid = {12690261},
language = {English},
rating = {0},
date-added = {2013-09-17T01:43:22GMT},
date-modified = {2015-03-17T13:42:23GMT},
abstract = {PURPOSE OF REVIEW:Exercise and passive motion exert reparative effects on inflamed joints, whereas excessive mechanical forces initiate cartilage destruction as observed in osteoarthritis. However, the intracellular mechanisms that convert mechanical signals into biochemical events responsible for cartilage destruction and repair remain paradoxical. This review summarizes how signals generated by mechanical stress may initiate repair or destruction of cartilage.

RECENT FINDINGS:Mechanical strain of low magnitude inhibits inflammation by suppressing IL-1beta and TNF-alpha-induced transcription of multiple proinflammatory mediators involved in cartilage degradation. This also results in the upregulation of proteoglycan and collagen synthesis that is drastically inhibited in inflamed joints. On the contrary, mechanical strain of high magnitude is proinflammatory and initiates cartilage destruction while inhibiting matrix synthesis. Investigations reveal that mechanical signals exploit nuclear factor-kappa B as a common pathway for transcriptional inhibition/activation of proinflammatory genes to control catabolic processes in chondrocytes. Mechanical strain of low magnitude prevents nuclear translocation of nuclear factor kappa B, resulting in the suppression of proinflammatory gene expression, whereas mechanical strain of high magnitude induces transactivation of nuclear factor kappa B, and thus proinflammatory gene induction.

SUMMARY:The beneficial effects of physiological levels of mechanical signals or exercise may be explained by their ability to suppress the signal transduction pathways of proinflammatory/catabolic mediators, while stimulating anabolic pathways. Whether these anabolic signals are a consequence of the inhibition of nuclear factor kappa B or are mediated via distinct anabolic pathways is yet to be elucidated.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=12690261&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Jake/Dropbox/Library.papers3/Articles/2003/Deschner/Curr%20Opin%20Clin%20Nutr%20Metab%20Care%202003%20Deschner.pdf},
file = {{Curr Opin Clin Nutr Metab Care 2003 Deschner.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/2003/Deschner/Curr Opin Clin Nutr Metab Care 2003 Deschner.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1097/01.mco.0000068964.34812.2b}}
}

@article{Klein:2007gx,
author = {Klein, Travis J and Chaudhry, Manu and Bae, Won C and Sah, Robert L},
title = {{Depth-dependent biomechanical and biochemical properties of fetal, newborn, and tissue-engineered articular cartilage.}},
journal = {Journal of Biomechanics},
year = {2007},
volume = {40},
number = {1},
pages = {182--190},
affiliation = {Department of Bioengineering, 9500 Gilman Dr., Mail Code 0412, University of California, San Diego, La Jolla, CA 92093-0412, USA.},
keywords = {Statified TE},
doi = {10.1016/j.jbiomech.2005.11.002},
pmid = {16387310},
language = {English},
rating = {0},
date-added = {2013-09-17T01:46:32GMT},
date-modified = {2015-03-17T13:42:23GMT},
abstract = {Adult articular cartilage has depth-dependent mechanical and biochemical properties which contribute to zone-specific functions. The compressive moduli of immature cartilage and tissue-engineered cartilage are known to be lower than those of adult cartilage. The objective of this study was to determine if such tissues exhibit depth-dependent compressive properties, and how these depth-varying properties were correlated with cell and matrix composition of the tissue. The compressive moduli of fetal and newborn bovine articular cartilage increased with depth (p<0.05) by a factor of 4-5 from the top 0.1 mm (28+/-13 kPa, 141+/-10 kPa, respectively) to 1 mm deep into the tissue. Likewise, the glycosaminoglycan and collagen content increased with depth (both p<0.001), and correlated with the modulus (both p<0.01). In contrast, tissue-engineered cartilage formed by either layering or mixing cells from the superficial and middle zone of articular cartilage exhibited similarly soft regions at both construct surfaces, as exemplified by large equilibrium strains. The properties of immature cartilage may provide a template for developing tissue-engineered cartilage which aims to repair cartilage defects by recapitulating the natural development and growth processes. These results suggest that while depth-dependent properties may be important to engineer into cartilage constructs, issues other than cell heterogeneity must be addressed to generate such tissues.},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0021929005005178},
local-url = {file://localhost/Users/Jake/Dropbox/Library.papers3/Articles/2007/Klein/J%20Biomech%202007%20Klein.pdf},
file = {{J Biomech 2007 Klein.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/2007/Klein/J Biomech 2007 Klein.pdf:application/pdf;J Biomech 2007 Klein.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/2007/Klein/J Biomech 2007 Klein.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1016/j.jbiomech.2005.11.002}}
}

