author = {Han, Sang-Kuy and Wouters, Wim and Clark, Andrea and Herzog, Walter},
title = {{Mechanically induced calcium signaling in chondrocytes in situ.}},
journal = {Journal of orthopaedic research : official publication of the Orthopaedic Research Society},
year = {2012},
volume = {30},
number = {3},
pages = {475--481},
month = mar,
affiliation = {Human Performance Laboratory, Faculty of Kinesiology, University of Calgary, 2500 University DR. N.W., Calgary, Alberta, Canada.},
doi = {10.1002/jor.21536},
