@article{Noriega:2007kc,
author = {Noriega, Sandra and Noriega, Sandra and Mamedov, Tarlan and Mamedov, Tarlan and Turner, Joseph A and Turner, Joseph A and Subramanian, Anu and Subramanian, Anu},
title = {{Intermittent applications of continuous ultrasound on the viability, proliferation, morphology, and matrix production of chondrocytes in 3D matrices.}},
journal = {Tissue engineering},
year = {2007},
volume = {13},
number = {3},
pages = {611--618},
month = mar,
affiliation = {Department of Chemical and Biomolecular Engineering, University of Nebraska, Lincoln, Nebraska 68588-0643, USA.},
doi = {10.1089/ten.2006.0130},
pmid = {17518607},
language = {English},
rating = {0},
date-added = {2013-09-17T01:42:01GMT},
date-modified = {2015-03-17T13:42:34GMT},
abstract = {Chondrocytes, the cellular component of the articular cartilage, have long been recognized as strain-sensitive cells, and have the ability to sense mechanical stimulation through surface receptors and intracellular signaling pathways. This strain-induced biological response of chondrocytes has been exploited to facilitate chondrocyte culture in in vitro systems; examples include the application of hydrostatic pressure, dynamic compression, hydrodynamic shear (i.e., rotating bioreactors), and low-intensity pulsed ultrasound (US). While the ability of US to influence chondrogenesis has been documented, the precise mechanisms of US-induced stimulation continue to be investigated. There remains a critical need to evaluate the impact of US on chondrocytes in 3D culture, which is a necessary microenvironment for maintaining the chondrocyte phenotype. In this study, a continuous US wave for predetermined time intervals was employed, as opposed to pulsed US used in previous studies, to stimulate chondrocytes seeded in 3D scaffolds. The chondrocytes (n = 6) were subjected to US stimulation as follows: 1.5 MHz for 161 seconds, 5.0 MHz for 51 seconds, and 8.5 MHz for 24 seconds, and the US signal was applied twice in a 24-hour period. Scaffolds that are not stimulated by US served as the control. Both the control and the US-stimulated groups were maintained in culture for 10 days, and at the conclusion of the culture period, chondrocytes were assayed for total DNA content, morphology, and cartilage-specific gene expression by reverse transcriptase polymerase chain reaction. Our results show that chondrocytes when stimulated with continuous US for predetermined time intervals possessed higher cellular viability (1.2 to 1.4 times) and higher levels of type II collagen and aggrecan mRNA expression when compared to controls.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=17518607&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Jake/Dropbox/Library.papers3/Articles/2007/Noriega/Tissue%20Eng.%202007%20Noriega.pdf},
file = {{Tissue Eng. 2007 Noriega.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/2007/Noriega/Tissue Eng. 2007 Noriega.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1089/ten.2006.0130}}
}

@article{Gray:1989vi,
author = {Gray, M L and Gray, M L and Pizzanelli, A M and Pizzanelli, A M and Lee, R C and Lee, R C and Grodzinsky, A J and Swann, D A and Swann, D A},
title = {{Kinetics of the chondrocyte biosynthetic response to compressive load and release.}},
journal = {Biochimica et biophysica acta},
year = {1989},
volume = {991},
number = {3},
pages = {415--425},
month = jun,
affiliation = {Department of Electrical Engineering and Computer Science, Massachusetts Institute of Technology, Cambridge 02139.},
pmid = {2730919},
language = {English},
rating = {0},
date-added = {2013-09-17T01:42:06GMT},
date-modified = {2015-03-17T13:42:34GMT},
abstract = {To gain insight regarding the rate at which cartilage tissue can sense and respond to a dynamic mechanical stimulus, we have examined the time-course of changes in biosynthetic activity following both the application and release of a static compressive stress. Cartilage harvested from the reserve zone of calf epiphyseal plate was subjected to unconfined static compressive stresses of 0, 0.25 and 0.5 MPa. Incorporation of [35S]sulfate and [3H]proline was measured during loading periods of less than 1 to 26 h and after preloading periods of 0.5, 2 or 12 h. During loading, total incorporation decreased to steady levels with time constants estimated to be 0.25-4 h (proline) and 1-5 h (sulfate). Proline incorporation exceeded control levels for 3 h after release of a 2 or 12 h preload. Sulfate incorporation remained depressed for at least 4 h after release of a 12 h preload and remained at control levels following release of 0.5 and 2 h preloads. We conclude that the modulation of proline incorporation by both loading and load release is faster than the modulation of sulfate incorporation. Furthermore, the response to unloading is not just the inverse of the response to loading; this nonlinearity suggests that the response to dynamic loading would not be determined simply by the time average component of the dynamic load.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=2730919&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Jake/Dropbox/Library.papers3/Articles/1989/Gray/Biochim.%20Biophys.%20Acta%201989%20Gray.pdf},
file = {{Biochim. Biophys. Acta 1989 Gray.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/1989/Gray/Biochim. Biophys. Acta 1989 Gray.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/2E307E61-0E09-424A-8E5C-2D7755CC3793}}
}

