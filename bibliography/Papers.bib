file = {{PLoS ONE 2010 Zhu.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/2010/Zhu/PLoS ONE 2010 Zhu.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1371/journal.pone.0015174}}
}

