@article{Mawatari:2010kb,
author = {Mawatari, Taro and Mawatari, Taro and Lindsey, Derek P and Lindsey, Derek P and Harris, Alex H S and Harris, Alex H S and Goodman, Stuart B and Goodman, Stuart B and Maloney, William J and Maloney, William J and Smith, Robert L and Smith, Robert L},
title = {{Effects of tensile strain and fluid flow on osteoarthritic human chondrocyte metabolism in vitro.}},
