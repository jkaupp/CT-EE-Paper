@article{Yang:2006ef,
author = {Yang, K G Auw and Saris, D B F and Geuze, R E and Helm, Y J M Van Der and Rijen, M H P Van and Verbout, A J and Dhert, Wouter J A and Creemers, L B},
title = {{Impact of expansion and redifferentiation conditions on chondrogenic capacity of cultured chondrocytes.}},
