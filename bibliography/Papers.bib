@article{Bondy:2001vk,
author = {Bondy, Kathleen N and Koenigseder, Lauretta A and Ishee, Jimmy H and Williams, Barbara G},
title = {{Psychometric Properties of the California Critical Thinking Tests}},
journal = {Journal of Nursing Measurement},
