@article{Fujisawa:1999wo,
author = {Fujisawa, T and Fujisawa, T and Hattori, T and Hattori, T and Takahashi, K and Takahashi, K and Kuboki, T and Kuboki, T and Yamashita, A and Yamashita, A and Takigawa, M and Takigawa, M},
title = {{Cyclic mechanical stress induces extracellular matrix degradation in cultured chondrocytes via gene expression of matrix metalloproteinases and interleukin-1.}},
journal = {Journal of biochemistry},
year = {1999},
volume = {125},
number = {5},
pages = {966--975},
month = may,
affiliation = {Department of Biochemistry and Molecular Dentistry, Okayama University Dental School, Okayama, 700-8525, Japan.},
pmid = {10220591},
language = {English},
rating = {0},
date-added = {2013-09-17T01:42:46GMT},
date-modified = {2015-03-17T13:42:35GMT},
abstract = {To clarify the mechanism of cartilage degradation induced by mechanical stress, we investigated the influence of cyclic tension force (CTF) on the metabolism of cultured chondrocytes. The chondrocytes were exposed to CTF using a Flexercell strain unit. Five or 15 kPa of high frequency CTF significantly inhibited the syntheses of DNA, proteoglycan, collagen, and protein. Fifteen kPa of high frequency CTF induced the expression of interleukin-1 (IL-1), matrix metalloproteinase (MMP)-2 and -9 mRNA, and increased the production of pro- and active-MMP-9. The degradation of proteoglycan was inhibited by and MMP inhibitor, indicating that MMPs are involved in the degradation of proteoglycans induced by high frequency CTF. Moreover, reducing the frequency of CTF from high to low decreased the inhibition of proteoglycan synthesis. These findings suggest that the CTF frequency is one of the key determinants of chondrocyte metabolism. Low magnitude CTF, whether high or low frequency, did not cause the gene expression of cartilage degradation factors, suggesting that this CTF magnitude causes only minor changes in the cartilage matrix. High magnitude and frequency CTF caused the gene expression of IL-1 and MMP-9, followed by increases in the production of MMP-2 and -9 proteins, suggesting that excessive and continuous cyclic mechanical stress induces the production of IL-1 and MMP-9, resulting in cartilage degradation.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=10220591&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Jake/Dropbox/Library.papers3/Articles/1999/Fujisawa/J.%20Biochem.%201999%20Fujisawa.pdf},
