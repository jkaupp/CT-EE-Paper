journal = {Osteoarthritis and Cartilage},
year = {2005},
volume = {13},
number = {9},
pages = {828--836},
month = sep,
affiliation = {George W. Woodruff School of Mechanical Engineering, Georgia Institute of Technology, Atlanta, GA 30332-0405, USA.},
