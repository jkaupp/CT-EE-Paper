@article{Jones:2007ud,
author = {Jones, A R C and Flannery, Carl R},
title = {{Bioregulation of lubricin expression by growth factors and cytokines.}},
journal = {Eur Cell Mater},
year = {2007},
volume = {13},
pages = {40--5; discussion 45},
affiliation = {Department of Women's Health {\&} Musculoskeletal Biology, Wyeth Research, Cambridge, MA 02140, USA.},
pmid = {17373642},
language = {English},
rating = {0},
date-added = {2013-09-17T01:42:46GMT},
date-modified = {2015-03-17T13:42:26GMT},
abstract = {Lubricin, also commonly referred to as superficial zone protein (SZP) and proteoglycan 4 (PRG4), is a multifaceted, cytoprotective glycoprotein that contributes to the boundary lubrication properties facilitating low friction levels at interfacing surfaces of articular cartilage. Biological processes effecting the gain or loss of lubricin function may therefore have important consequences relevant to joint physiology and pathology. Herein, we describe experiments conducted to extend our understanding of the influence of various cytokines and growth factors on lubricin gene expression and protein secretion in synovial tissues. Exposure of synoviocytes, chondrocytes and cartilage explants to proinflammatory cytokines such as interleukin-1 (IL-1) and tumor necrosis factor-alpha (TNF-alpha) results in a marked reduction in the expression and/or abundance of secreted lubricin, with corresponding alterations in the amounts of cartilage-associated (boundary) lubricin. Conversely, treatment with transforming growth factor-beta (TGF-beta) significantly upregulates lubricin synthesis, secretion and cartilage boundary association. Oncostatin M also appears to be capable of modulating lubricin metabolism, with the potential to induce lubricin synthesis by chondrocytes. Collectively, the results of studies on cytokine and growth factor regulation of lubricin biosynthesis and biodistribution may help provide new insights and therapeutic perspectives for promoting joint function.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=17373642&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Jake/Dropbox/Library.papers3/Articles/2007/Jones/Eur%20Cell%20Mater%202007%20Jones.pdf},
file = {{Eur Cell Mater 2007 Jones.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/2007/Jones/Eur Cell Mater 2007 Jones.pdf:application/pdf}},
uri = {\url{papers3://publication/uuid/9FF74B0B-FB92-4AF8-8548-9A522C1A47BF}}
}

