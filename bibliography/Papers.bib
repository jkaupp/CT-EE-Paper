@article{Alhadlaq:2007jt,
author = {Alhadlaq, Hisham A and Xia, Yang and Hansen, Fay M and Les, Clifford M and Lust, George},
title = {{Morphological changes in articular cartilage due to static compression: polarized light microscopy study.}},
journal = {Connective Tissue Research},
year = {2007},
volume = {48},
number = {2},
pages = {76--84},
affiliation = {Department of Physics and Center for Biomedical Research, Oakland University, Rochester, Michigan 48309, USA.},
doi = {10.1080/03008200601130950},
