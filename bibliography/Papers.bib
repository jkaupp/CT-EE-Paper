file = {{Tissue Eng. 2006 Yang.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/2006/Yang/Tissue Eng. 2006 Yang.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1089/ten.2006.12.2435}}
}

