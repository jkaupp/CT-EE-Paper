file = {{J. Orthop. Res. 1999 Schumacher.pdf:/Users/Jake/Dropbox/Library.papers3/Articles/1999/Schumacher/J. Orthop. Res. 1999 Schumacher.pdf:application/pdf}},
uri = {\url{papers3://publication/doi/10.1002/jor.1100170117}}
}

