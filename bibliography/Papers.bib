@article{DeMaria:2011go,
author = {De Maria, Carmelo and Giusti, Serena and Mazzei, Daniele and Crawford, Aileen and Ahluwalia, Arti},
title = {{Squeeze pressure bioreactor: a hydrodynamic bioreactor for noncontact stimulation of cartilage constructs.}},
journal = {Tissue engineering. Part C, Methods},
year = {2011},
volume = {17},
number = {7},
pages = {757--764},
month = jul,
affiliation = {Interdepartmental Research Center E. Piaggio, University of Pisa, Pisa, Italy.},
doi = {10.1089/ten.TEC.2011.0002},
